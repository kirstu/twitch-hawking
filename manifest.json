{
  "manifest_version": 2,

  "name": "Twitch Hawking",
  "description": "This extension will tell the truth",
  "version": "1.0",

  "browser_action": {
   "default_icon": "speech.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["main.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  }
}
