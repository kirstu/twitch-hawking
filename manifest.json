{
  "manifest_version": 2,
  "name": "Twitch Hawkings",
  "description": "This extension will tell the truth",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["main.js"]
  }],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
